{
    "extends": [
        "astro/tsconfigs/base"
    ],
    "esModuleInterop": true,
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "paths": {
            "game/*": ["./game/*"],
            "app/*": ["./app/src/*"],
            "lib/*": ["./lib/*"]
        }
    },
    "exclude": [
        "lib/**/*"
    ]
}